    "build": "npm run build:js && npm run build:millimetr"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@millimetr/cli": "^1.1.4",
    "npm-run-all": "^4.1.5"
  }
}
